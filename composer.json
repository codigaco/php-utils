{
  "name": "codigaco/php-utils",
  "description": "PHP Utils from Codigaco!",
  "autoload": {
    "files": [
      "vendor/autoload.php"
    ],
    "psr-4": {
      "Codigaco\\PhpUtils\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Codigaco\\PhpUtils\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.4",
    "symfony/property-info": "^5.4.10"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "platform": {
      "php": "7.4.30",
      "ext-something": "1"
    }
  }
}

